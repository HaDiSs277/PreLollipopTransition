<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.albinmathew.prelollipoptransition">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.albinmathew.prelollipoptransition.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.albinmathew.prelollipoptransition.SubActivity"
            android:label="@string/title_activity_sub" >
        </activity>
        <activity
            android:name="com.albinmathew.prelollipoptransition.SubActivity2"
            android:label="@string/title_activity_sub" >
        </activity>
    </application>

</manifest>
